    <!-- http://purl.org/ontology/bibo/Thesis -->

    <owl:Class rdf:about="http://purl.org/ontology/bibo/Thesis"/>
    


    <!-- http://xmlns.com/foaf/0.1/Agent -->

    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/CERFLevel -->

    <owl:Class rdf:about="https://w3id.org/ecodigit/ontology/eas/CERFLevel">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ecodigit/ontology/eas/Level"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta i sei livelli (A1, A2, B1, B2, C1, C2) previsti dal Quadro comune europeo di riferimento per la conoscenza delle lingue (QCER), in inglese Common European Framework of Reference for Languages (CEFR).</rdfs:comment>
        <rdfs:label xml:lang="en">CERF Level</rdfs:label>
        <rdfs:label xml:lang="it">Livello CERF</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/Certification -->

    <owl:Class rdf:about="https://w3id.org/ecodigit/ontology/eas/Certification">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/l0/Object"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ecodigit/ontology/eas/attests"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/ecodigit/ontology/eas/Level"/>
                            <rdf:Description rdf:about="https://w3id.org/ecodigit/ontology/eas/LevelInActivity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ecodigit/ontology/eas/issueDate"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">Questa classe rappresenta la documentazione, che può essere istituzionale o autodichiarata, relativa alla Certificazione del Livello di Istruzione e Competenza di una persona.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents the documentation, which can be institutional or self-declared, related to the Certification of the Level of Education and Expertise of a person.</rdfs:comment>
        <rdfs:label xml:lang="en">Certification</rdfs:label>
        <rdfs:label>Certificazione</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/Examination -->

    <owl:Class rdf:about="https://w3id.org/ecodigit/ontology/eas/Examination">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/TI/TimeIndexedEvent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ecodigit/ontology/eas/leadsTo"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ecodigit/ontology/eas/Certification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ecodigit/ontology/grade/hasGrade"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ecodigit/ontology/grade/Grade"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ecodigit/ontology/eas/hasTrainingOutcome"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/ontology/bibo/Thesis"/>
