    <!-- https://w3id.org/ecodigit/ontology/eas/issueDate -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/issueDate">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/TI/date"/>
        <rdfs:domain rdf:resource="https://w3id.org/ecodigit/ontology/eas/Certification"/>
