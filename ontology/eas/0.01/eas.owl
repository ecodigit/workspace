     -->

    


    <!-- https://w3id.org/ecodigit/ontology/eas/attends -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/attends">
        <owl:inverseOf rdf:resource="https://w3id.org/ecodigit/ontology/eas/isAttendedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:range rdf:resource="https://w3id.org/ecodigit/ontology/eas/Training"/>
        <rdfs:comment xml:lang="it">Questa proprietà permette di associare una Persona con l&apos;Esperienza formativa da essa frequentata.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property associates a Person with the attended Training.</rdfs:comment>
        <rdfs:label xml:lang="en">attends</rdfs:label>
        <rdfs:label xml:lang="it">frequenta</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/attests -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/attests">
        <owl:inverseOf rdf:resource="https://w3id.org/ecodigit/ontology/eas/isAttestedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/ecodigit/ontology/eas/Certification"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ecodigit/ontology/eas/Level"/>
                    <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/EducationLevel"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Questa proprietà permette di associare una Certificazione con il Livello di istruzione o competenza attestato.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property associates a Certification with the attested Level of Education or Expertise.</rdfs:comment>
        <rdfs:label xml:lang="it">attesta</rdfs:label>
        <rdfs:label xml:lang="en">attests</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/hasActivity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/hasActivity">
        <rdfs:domain rdf:resource="https://w3id.org/ecodigit/ontology/eas/LevelInActivity"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Activity"/>
        <rdfs:label xml:lang="it">Associa il Livello in Attività con l&apos;attività a cui si riferisce.</rdfs:label>
        <rdfs:label xml:lang="it">ha attività</rdfs:label>
        <rdfs:label xml:lang="en">has activity</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/hasCertification -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/hasCertification">
        <owl:inverseOf rdf:resource="https://w3id.org/ecodigit/ontology/eas/isCertificationOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/ecodigit/ontology/eas/Certification"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa una Entità alla sua Certificazione sia essa istituzionale o autodichiarata.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property associates an Entity with its Certification whether it is institutional or self-declared.</rdfs:comment>
        <rdfs:label xml:lang="it">ha certificazione</rdfs:label>
        <rdfs:label xml:lang="en">has certification</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/hasExpertise -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/hasExpertise">
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:range rdf:resource="https://w3id.org/ecodigit/ontology/eas/ExpertiseAndSkill"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa una Persona con la sua Esperienza o Competenza.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property associates a Person with his Expertise or Competence.</rdfs:comment>
        <rdfs:label xml:lang="it">ha esperienza</rdfs:label>
        <rdfs:label xml:lang="en">has expertise</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <prov:wasInfluencedBy rdf:resource="http://purl.org/ontology/cco/core#expertise"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/hasLevel -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/hasLevel">
        <rdfs:domain rdf:resource="https://w3id.org/ecodigit/ontology/eas/LevelInActivity"/>
        <rdfs:range rdf:resource="https://w3id.org/ecodigit/ontology/eas/Level"/>
        <rdfs:comment xml:lang="it">Associa un Livello in Attività con il Livello a cui si riferisce.</rdfs:comment>
        <rdfs:label xml:lang="it">ha livello</rdfs:label>
        <rdfs:label xml:lang="en">has level</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/hasTrainingOutcome -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/hasTrainingOutcome">
        <owl:inverseOf rdf:resource="https://w3id.org/ecodigit/ontology/eas/isTrainingOutcomeOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ecodigit/ontology/eas/Examination"/>
                    <rdf:Description rdf:about="https://w3id.org/ecodigit/ontology/eas/Training"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment xml:lang="it">Questa proprietà associa un&apos;Esperienza formativa o un Esame con il Risultato della formazione, per esempio una Tesi.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property associates a Training experience or an Examination with the Training outcome, e.g. a Thesis.</rdfs:comment>
        <rdfs:label xml:lang="it">ha risultato della formazione</rdfs:label>
        <rdfs:label xml:lang="en">has training outcome</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/includesAnExamination -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/includesAnExamination">
        <rdfs:domain rdf:resource="https://w3id.org/ecodigit/ontology/eas/Training"/>
        <rdfs:range rdf:resource="https://w3id.org/ecodigit/ontology/eas/Examination"/>
        <rdfs:comment xml:lang="it">La proprietà permette di associare una Esperienza formativa con gli esami necessari per il suo completamento.</rdfs:comment>
        <rdfs:comment xml:lang="en">The property associates a Training with the Examinations required for its completion.</rdfs:comment>
        <rdfs:label xml:lang="it">include un esame</rdfs:label>
        <rdfs:label xml:lang="en">includes an examination</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/isAttendedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/isAttendedBy">
        <rdfs:domain rdf:resource="https://w3id.org/ecodigit/ontology/eas/Training"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:comment xml:lang="it">Questa è la proprietà inversa di &apos;frequenta&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="en">This is the inverse object property of &apos;attends&apos;.</rdfs:comment>
        <rdfs:label xml:lang="en">is attended by</rdfs:label>
        <rdfs:label xml:lang="it">è frequentato da</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/isAttestedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/isAttestedBy">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ecodigit/ontology/eas/Level"/>
                    <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/EducationLevel"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/ecodigit/ontology/eas/Certification"/>
        <rdfs:comment xml:lang="it">Questa è la proprietà inversa di &apos;attesta&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="en">This is the inverse object property of &apos;attests&apos;.</rdfs:comment>
        <rdfs:label xml:lang="en">is attested by</rdfs:label>
        <rdfs:label xml:lang="it">è attestato da</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/isCertificationOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/isCertificationOf">
        <rdfs:domain rdf:resource="https://w3id.org/ecodigit/ontology/eas/Certification"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="it">Questa è la proprietà inversa di &apos;ha certificazione&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="en">This is the inverse object property of &apos;has certification&apos;.</rdfs:comment>
        <rdfs:label xml:lang="en">is certification of</rdfs:label>
        <rdfs:label xml:lang="it">è certificazione di</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="it">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/isTrainingOutcomeOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/isTrainingOutcomeOf">
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ecodigit/ontology/eas/Examination"/>
                    <rdf:Description rdf:about="https://w3id.org/ecodigit/ontology/eas/Training"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Questa è la proprietà inversa di &apos;ha risultato della formazione&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="en">This is the inverse object property of &apos;has training outcome&apos;.</rdfs:comment>
        <rdfs:label xml:lang="en">is training outcome of</rdfs:label>
        <rdfs:label xml:lang="it">è risultato della formazione di</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/issuedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/issuedBy">
        <owl:inverseOf rdf:resource="https://w3id.org/ecodigit/ontology/eas/issues"/>
        <rdfs:domain rdf:resource="https://w3id.org/ecodigit/ontology/eas/Certification"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa una Certificazione con l&apos;Organizzazione che la rilascia.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property associates a Certification with the issuing Organization.</rdfs:comment>
        <rdfs:label xml:lang="en">certification issued by</rdfs:label>
        <rdfs:label xml:lang="it">certificazione rilasciata da</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/issues -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/issues">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
        <rdfs:range rdf:resource="https://w3id.org/ecodigit/ontology/eas/Certification"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un&apos;Organizzazione con la Certificazione che essa rilascia.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property associates an Organization with the Certification that it issues.</rdfs:comment>
        <rdfs:label xml:lang="en">issues certification</rdfs:label>
        <rdfs:label xml:lang="it">rilascia certificazione</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/leadsTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/leadsTo">
        <rdfs:domain rdf:resource="https://w3id.org/ecodigit/ontology/eas/Examination"/>
        <rdfs:range rdf:resource="https://w3id.org/ecodigit/ontology/eas/Certification"/>
        <rdfs:comment xml:lang="it">Questa proprietà permette di associare la situazione del superamento di un Esame con la Certificazione ottenuta.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property allows you to associate the situation of passing an Exam with the Certification obtained.</rdfs:comment>
        <rdfs:label xml:lang="en">leads to the achievement of</rdfs:label>
        <rdfs:label xml:lang="it">porta al conseguimento di</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/providedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/providedBy">
        <owl:inverseOf rdf:resource="https://w3id.org/ecodigit/ontology/eas/provides"/>
        <rdfs:domain rdf:resource="https://w3id.org/ecodigit/ontology/eas/Training"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
        <rdfs:comment xml:lang="it">Questa proprietà permette di associare una situazione di Formazione con l&apos;Organizzazione che la eroga.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property associates a Training situation with the Organization providing it.</rdfs:comment>
        <rdfs:label xml:lang="it">offerta da</rdfs:label>
        <rdfs:label xml:lang="en">provided by</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ecodigit/ontology/eas/provides -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/provides">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
        <rdfs:range rdf:resource="https://w3id.org/ecodigit/ontology/eas/Training"/>
        <rdfs:comment xml:lang="it">Questa è la proprietà inversa di &apos;è offerta da&apos;.</rdfs:comment>
        <rdfs:comment xml:lang="en">This is the inverse object property of &apos;provided by&apos;.</rdfs:comment>
        <rdfs:label xml:lang="it">offre</rdfs:label>
        <rdfs:label xml:lang="en">provides</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


