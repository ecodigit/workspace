        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ecodigit/ontology/eas/hasActivity"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/italia/onto/l0/Activity"/>
            </owl:Restriction>
