    <!-- https://w3id.org/ecodigit/ontology/eas/hasTrainingOutcome -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ecodigit/ontology/eas/hasTrainingOutcome">
        <owl:inverseOf rdf:resource="https://w3id.org/ecodigit/ontology/eas/isTrainingOutcomeOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ecodigit/ontology/eas/Examination"/>
                    <rdf:Description rdf:about="https://w3id.org/ecodigit/ontology/eas/Training"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un&apos;Esperienza formativa o un Esame con qualsiasi cosa sia il risultato della formazione (ad es. una Tesi).</rdfs:comment>
        <rdfs:comment xml:lang="en">This property associates a Training experience or an Examination  with anything is the result of the training (e.g. a Thesis).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/ecodigit/ontology/eas/"/>
        <rdfs:label xml:lang="it">ha risultato della formazione</rdfs:label>
        <rdfs:label xml:lang="en">has training outcome</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


